# Airflow
AIRFLOW__CORE__EXECUTOR=LocalExecutor
AIRFLOW__CORE__FERNET_KEY=your-fernet-key-here
AIRFLOW__CORE__LOAD_EXAMPLES=false
AIRFLOW__WEBSERVER__SECRET_KEY=your-webserver-secret-key
AIRFLOW_ADMIN_USER=admin
AIRFLOW_ADMIN_PASSWORD=admin

# Airflow Database
AIRFLOW__DATABASE__SQL_ALCHEMY_CONN=postgresql+psycopg2://airflow:airflow@postgres-airflow:5432/airflow

# Warehouse Database
WAREHOUSE_DB_HOST=postgres-warehouse
WAREHOUSE_DB_PORT=5432
WAREHOUSE_DB_NAME=warehouse
WAREHOUSE_DB_USER=warehouse
WAREHOUSE_DB_PASSWORD=warehouse
WAREHOUSE_DATABASE_URL=postgresql+psycopg2://warehouse:warehouse@postgres-warehouse:5432/warehouse

# Kafka
KAFKA_BOOTSTRAP_SERVERS=kafka:9092
KAFKA_TOPIC_TRANSACTIONS=transactions
KAFKA_CONSUMER_GROUP=etl-consumer

# Producer
PRODUCER_INTERVAL_MIN=1.0
PRODUCER_INTERVAL_MAX=2.0

# Logging
LOG_LEVEL=INFO
